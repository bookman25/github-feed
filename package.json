{
  "name": "github-feed",
  "version": "2.1.0",
  "description": "Automate checking of common repos for updates",
  "main": "electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bookman25/github-feed.git"
  },
  "devDependencies": {
    "@types/aphrodite": "^0.5.10",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "@types/node-schedule": "0.0.34",
    "@types/nodemailer": "^4.6.0",
    "@types/react": "*",
    "@types/react-dom": "*",
    "coveralls": "^2.11.15",
    "electron": "^2.0.8",
    "electron-builder": "^19.33.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "fbjs-scripts": "^0.8.3",
    "jest": "^22.0.0",
    "minicat": "^1.0.0",
    "react-test-renderer": "^16.4.0",
    "ts-jest": "^22.4.6",
    "tslib": "^1.9.1",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-misc-rules": "^3.3.3",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@octokit/rest": "^15.7.0",
    "aphrodite": "^1.1.0",
    "node-notifier": "^5.2.1",
    "node-schedule": "^1.1.1",
    "nodemailer": "^4.6.5",
    "react": "*",
    "react-dom": "*"
  },
  "scripts": {
    "ci": "yarn lint && yarn test --coverage && yarn coveralls",
    "cleanfiles": "node scripts/clean",
    "coveralls": "minicat coverage/lcov.info | coveralls && rm -rf ./coverage",
    "lint": "tslint --exclude \"node_modules/**\" --exclude \"**/*.d.ts\" **/*.ts **/*.tsx",
    "pack": "tsc && build --dir",
    "dist": "tsc && build --x64",
    "start": "tsc && electron .",
    "test": "jest --config jest.json",
    "watch-test": "yarn test --watch"
  },
  "author": "Bryan Nicholls",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bookman25/github-feed/issues"
  },
  "build": {
    "appId": "github.feed",
    "win": {
      "icon": "electron/rss.ico"
    }
  }
}
